<h1>Listing professionals</h1>

<table>
  <tr>
    <th>Area of service</th>
    <th>Longitude</th>
    <th>Latitude</th>
    <th>Company name</th>
    <th>Email</th>
    <th>Phonenumber</th>
    <th>Fax</th>
    <th>Ptype</th>
    <th>Company num</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @professionals.each do |professional| %>
  <tr>
    <td><%= professional.area_of_service %></td>
    <td><%= professional.longitude %></td>
    <td><%= professional.latitude %></td>
    <td><%= professional.company_name %></td>
    <td><%= professional.email %></td>
    <td><%= professional.phonenumber %></td>
    <td><%= professional.fax %></td>
    <td><%= professional.ptype %></td>
    <td><%= professional.company_num %></td>
    <td><%= link_to 'Show', professional %></td>
    <td><%= link_to 'Edit', edit_professional_path(professional) %></td>
    <td><%= link_to 'Destroy', professional, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

